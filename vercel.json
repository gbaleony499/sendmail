{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
     
      {
        "src": "/assets/datas/data.php",
        "dest": "/public/assets/datas/data.php"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
}
